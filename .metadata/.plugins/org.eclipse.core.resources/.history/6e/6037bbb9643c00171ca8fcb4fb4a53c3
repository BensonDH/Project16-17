package asteroids.programs.expressions;

import asteroids.part3.programs.SourceLocation;
import asteroids.programs.Program;

public class DoubleLiteralExpression extends Expression implements Literal {

	public DoubleLiteralExpression(Double value) {
		super(null);
		this.value = value;
	}

	@Override
	public Literal eval(Program parentProgram) {
		return this;
	}

	@Override
	public Double getValue() {
		return this.value;
	}

	/**
	 * Variable registering the value of this DoubleLiteral
	 */
	private final Double value;
}
